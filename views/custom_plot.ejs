<br>
<input type="text" style="width: 80%;" id="reqColumnsInp" />

<div id="dcPlotsDiv" style="margin-top: 8px"></div>

<table id="dcTable" name="dcTable" style="margin-top: 8px"></table>
<script>
    var utilsObj_ = <%- JSON.stringify(utils)%>;
    var utilsNetSchObj_ = <%- JSON.stringify(utils_net_sch)%>;
    var revs_ = <%- JSON.stringify(revisions)%>;
    var dateStr_ = <%- JSON.stringify(date_str)%>;
</script>
<script src="./libs/js/jquery.min.js"></script>
<script src="./libs/js/plotly.min.js"></script>
<script src="domUtils.js"></script>
<script src="revision.js"></script>
<script src="custom_plot.js"></script>
</body>
</html>